{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmitOnError": false,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/__tests__/**/*",
    "scripts/**/*"
  ]
}